source 'http://rubygems.org'

gem 'rack'


# Bundle gems for the local environment. Make sure to
# put test-only gems in this group so their generators
# and rake tasks are available in development mode:
group :development, :test do
    gem 'cucumber'
    gem 'capybara'
    gem 'culerity'
    gem 'pickle'
    gem 'faker'
    gem 'launchy'
    gem 'spork'
    gem 'rspec'
    gem 'ruby-debug19'
end
